{
  "name": "construcao-sftw-t1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node build/src/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "tsoa:gen": "tsoa routes && tsoa swagger"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.11.2",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.6",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1"
  }
}