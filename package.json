{
  "name": "construcao-sftw-t1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node build/src/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "tsoa:gen": "tsoa routes && tsoa swagger",
    "test": "mocha",
    "test-dev": "nodemon --watch . --ext ts --exec \"mocha -r ts-node/register test/**/*.test.ts\""
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "dependencies": {
    "@keycloak/keycloak-admin-client": "^15.0.2",
    "axios": "^0.21.4",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "keycloak-admin-client": "^0.12.0",
    "mocha": "^9.1.2",
    "mongoose": "^6.0.8",
    "morgan": "~1.9.1",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.11.2",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.6",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "chai": "^4.3.4",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "request": "^2.88.2",
    "should": "^13.2.3",
    "ts-node": "^10.2.1"
  }
}
